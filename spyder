import openai

# Set up the OpenAI API client
openai.api_key = "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"


# Set up the model and prompt
model_engine = "text-davinci-003"
prompt = "give me a grep command to select lines starting with letter A"

# Generate a response
completion = openai.Completion.create(
    engine=model_engine,
    prompt=prompt,
    max_tokens=1024,
    n=1,
    stop=None,
    temperature=0.5,
)

response = completion.choices[0].text
print(response)
